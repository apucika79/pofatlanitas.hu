# Local Supabase credentials (replace the placeholders with the output of `supabase start`)
SUPABASE_URL=http://127.0.0.1:54321
SUPABASE_ANON_KEY=REPLACE_WITH_LOCAL_ANON_KEY
SUPABASE_SERVICE_ROLE_KEY=REPLACE_WITH_LOCAL_SERVICE_ROLE_KEY
SUPABASE_AUTH_REDIRECT_URL=http://localhost:4173/admin.html
VIDEO_BUCKET=videos

# Front-end build variables (Vite/ESM-compatible)
VITE_SUPABASE_URL=http://127.0.0.1:54321
VITE_SUPABASE_ANON_KEY=REPLACE_WITH_LOCAL_ANON_KEY
VITE_SUPABASE_AUTH_REDIRECT_URL=http://localhost:4173/admin.html
VITE_VIDEO_BUCKET=videos
# Never expose the service role key to client bundles.
